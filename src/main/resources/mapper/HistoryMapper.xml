<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seu.film.mapper.HistoryMapper">
    <!-- int addHistory(History history) -->
    <insert id="addHistory" parameterType="History">
        INSERT INTO history (userid,filmid,date)
        VALUES (#{userid},#{filmid},#{date})
    </insert>
</mapper>